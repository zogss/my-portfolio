{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "target": "esnext",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "exclude": [".*.js", "node_modules", "public", "static", ".cache"],
  "include": [
    "src",
    "config",
    "gatsby-node.ts",
    "gatsby-config.ts",
    "gatsby-browser.tsx",
    "gatsby-ssr.tsx"
  ]
}
